{
  "name": "bridget-portfolio",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "npx eleventy --serve --quiet",
    "build": "ELEVENTY_ENV=production npx eleventy",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css,scss,md,json,njk,html}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,css,scss,md,json,njk,html}\"",
    "lint:js": "eslint .",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint": "npm run lint:js && npm run lint:css",
    "precommit": "npm run format:check && npm run lint",
    "lhci": "lhci autorun",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@lhci/cli": "^0.15.1",
    "eslint": "^8.57.1",
    "husky": "^9.1.7",
    "prettier": "^3.7.4",
    "sharp": "^0.34.5",
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^39.0.1"
  }
}
